<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <title>Avatar | CYBER.ME</title>
    <link rel="stylesheet" href="/styles.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>

<body class="cyber-frame">
    <div class="signin-container">
        <div class="policy-top">
            <div class="row white-box cyberme-pc">
                <div class="col-sm-9">
                    <img class="cyberme-feed-small" src="/images/feed-logo.png" alt="Feed Logo" />
                </div>
                <div class="col-sm-3">
                    <a href="signin"><img class="cyberme-back" src="/images/back.png" alt="Vector Icon" /></a>
                </div>
            </div>

            <div class="row white-box cyberme-mobile">
                <div class="col-sm-12">
                    <img class="cyberme-feed-small" src="/images/feed-logo.png" alt="Feed Logo" />
                </div>
            </div>
            <div class="row white-box avatar-space">
                <!-- ตัวอย่าง 12 รูป Avatar -->
                <div class="col-4 col-md-2">
                    <a href="/camera/1">
                        <img src="/avatar/1.jpg" class="img-fluid" alt="Avatar 1">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/2">
                        <img src="/avatar/2.jpg" class="img-fluid" alt="Avatar 2">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/3">
                        <img src="/avatar/3.jpg" class="img-fluid" alt="Avatar 3">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/4">
                        <img src="/avatar/4.jpg" class="img-fluid" alt="Avatar 4">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/5">
                        <img src="/avatar/5.jpg" class="img-fluid" alt="Avatar 5">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/6">
                        <img src="/avatar/6.jpg" class="img-fluid" alt="Avatar 6">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/7">
                        <img src="/avatar/7.jpg" class="img-fluid" alt="Avatar 7">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/8">
                        <img src="/avatar/8.jpg" class="img-fluid" alt="Avatar 8">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/9">
                        <img src="/avatar/9.jpg" class="img-fluid" alt="Avatar 9">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/10">
                        <img src="/avatar/10.jpg" class="img-fluid" alt="Avatar 10">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/11">
                        <img src="/avatar/11.jpg" class="img-fluid" alt="Avatar 11">
                    </a>
                </div>
                <div class="col-4 col-md-2">
                    <a href="/camera/12">
                        <img src="/avatar/12.jpg" class="img-fluid" alt="Avatar 12">
                    </a>
                </div>
            </div>
        </div>
    </div>

    <!-- Firebase SDK -->
    <script src="https://www.gstatic.com/firebasejs/9.17.1/firebase-app-compat.js"></script>
    <script src="https://www.gstatic.com/firebasejs/9.17.1/firebase-auth-compat.js"></script>
    <script src="https://www.gstatic.com/firebasejs/9.17.1/firebase-firestore-compat.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>

</html>